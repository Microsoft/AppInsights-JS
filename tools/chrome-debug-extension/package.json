{
  "name": "@microsoft/applicationinsights-web-chrome-debug-extension",
  "version": "0.0.1",
  "description": "A browser extension that provides a real time view of what's happening in Application Insights including what telemetry is being logged by the web application",
  "main": "index.ts",
  "module": "index.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run build:esm && npm run build:browser",
    "build:esm": "grunt chromedebugextension",
    "build:browser": "npx rollup -c",
    "rebuild": "npm run build",
    "test": "",
    "perftest": "",
    "publishPackage": "npm publish",
    "copy": "cp ./images ./dist-esm"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@microsoft/1ds-core-js": "3.1.7",
    "@microsoft/dynamicproto-js": "^1.1.4",
    "@microsoft/applicationinsights-shims": "^2.0.0",
    "@microsoft/load-themed-styles": "1.10.214",
    "@uifabric/merge-styles": "7.19.2",
    "@uifabric/styling": "7.19.1",
    "@uifabric/utilities": "7.33.5",
    "babel-preset-react-app": "9.0.2",
    "file-saver": "^2.0.0",
    "iterall": "^1.2.2",
    "object-assign": "4.1.1",
    "office-ui-fabric-react": "7.176.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "scheduler": "~0.11.2"
  },
  "devDependencies": {
    "@microsoft/applicationinsights-rollup-es3": "^1.1.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/chrome": "0.0.148",
    "@types/file-saver": "~2.0.1",
    "@types/node": "12.7.4",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "ansi-regex": "4.1.0",
    "autoprefixer": "9.4.5",
    "grunt": "^1.4.1",
    "rollup": "^2.32.0",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslint": "5.20.0",
    "tslint-microsoft-contrib": "6.1.0",
    "tslint-react": "4.0.0",
    "typescript": "4.0.3"
  }
}
