        "webpack-command": "^0.4.1"
    },
    "dependencies": {
        "@microsoft/applicationinsights-core-js": "^1.0.0-beta.3",
        "@microsoft/applicationinsights-common": "^1.0.0-beta.10",
        "@microsoft/applicationinsights-analytics-js": "^1.0.0-beta.8",
        "@microsoft/applicationinsights-channel-js": "^1.0.0-beta.9",
