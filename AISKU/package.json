    "description": "Microsoft Application Insights Javascript SDK API 1.0 beta",
    "main": "dist/applicationinsights-web.js",
    "module": "dist-esm/applicationinsights-web.js",
    "types": "dist-esm/applicationinsights-web.d.ts",
